
@{
    ViewData["Title"] = "IncomeHistory";
    Layout = "~/Views/Shared/_AdminLayout1.cshtml";
}
<link href="~/css/Record.css" rel="stylesheet" />

<div class="app">
    <div class="article">
        <div class="nav-bar" style="height: 60px;">
            <div class="nav-bar-fixed">
                <div class="navbar-content">
                    <a href="@Url.Action("Income","User")" style="text-decoration:none; margin-left:10px;">
                        <i class="fas fa-chevron-left" style="vertical-align: middle;"></i>
                        <span class="back-text" style="vertical-align: middle; position: relative;">Back</span>
                    </a>
                    <div class="title">
                        <div style="color: black;">Records</div>
                    </div>
                </div>
            </div>
        </div>
        <div class="article-wrap">
            <div class="article-content">
                <div class="member-table">
                    <table>
                        <thead>
                            <tr>
                                <th id="id-column" style="width: 10%;">ID</th>
                                <th id="amount-column" style="width: 20%;">Amount</th>
                                <th id="datetime-column" style="width: 30%;">Date & Time</th>
                                <th id="status-column" style="width: 20%;">Status</th>
                            </tr>
                        </thead>
                        <tbody id="withdrawal-records">
                        </tbody>
                    </table>
                    <p class="no-data-text">No more</p>
                </div>
            </div>

        </div>
    </div>
</div>

<script src="~/js/User/IncomeHistory.js"></script>
